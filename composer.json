{
    "name": "instantjay/symfdemo",
    "description": "Minimal demo of Symfony, the service container and more.",
    "type": "project",
    "require": {
        "php": "^5.6.0",
        "symfony/dependency-injection": "^3.4.0",
        "doctrine/orm": "^2.5.14",
        "symfony/dotenv": "^3.4.15",
        "symfony/filesystem": "^3.4.0",
        "symfony/config": "^3.4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7.27",
        "squizlabs/php_codesniffer": "^3.3.1",
        "mockery/mockery": "^1.1.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "John Jensen",
            "email": "john@jenseninnovations.se"
        }
    ],
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    }
}
