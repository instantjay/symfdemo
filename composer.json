{
    "name": "instantjay/symfdemo",
    "description": "Minimal demo of Symfony, the service container and more.",
    "type": "project",
    "require": {
        "php": "^7.1",
        "symfony/dependency-injection": "^5.0",
        "doctrine/orm": "^2.7",
        "symfony/dotenv": "^5.0",
        "symfony/filesystem": "^5.0",
        "symfony/config": "^5.0",
        "ext-pdo_mysql": "^7.3"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.5",
        "mockery/mockery": "^1.3",
        "phpunit/phpunit": "^9.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "John Jensen",
            "email": "john@jenseninnovations.se"
        }
    ],
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    }
}
